PYTHONPATH="{{ python_path }}"
PIPPATH="{{ pip_path }}"
SOARDIR="{{ soar_dir }}"
SOARPATH="{{ soar_path }}"
WORKSPACEPATH="{{ workspace_path }}"
STORAGEPATH="{{ storage_path }}"

alias updatesoar="cd $SOARDIR && git reset --hard HEAD && git pull && $PYTHONPATH $SOARPATH configure -r"
alias soarupdate="cd $SOARDIR && git reset --hard HEAD && git pull && $PYTHONPATH $SOARPATH configure -r"

alias refreshconfig="$PYTHONPATH $SOARPATH configure -r"

alias copyconfig="$PYTHONPATH $SOARPATH copy config"
alias configcopy="$PYTHONPATH $SOARPATH copy config"
